    "@tsconfig/node24": "^24.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.21",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.2.1",
